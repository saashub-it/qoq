{
  "name": "@saashub/qoq-eslint-v9-js-jest",
  "version": "1.1.5",
  "description": "Eslint flat config template for JS + Jest",
  "keywords": [
    "eslint JavaScript + Jest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-js-jest",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-js-jest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "bin": {
    "@saashub/qoq-eslint-v9-js-jest": "./bin/bin.cjs"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "scripts": {
    "build": "rimraf ./lib && rollup -c rollup.config.mjs --silent && rimraf ./bin && rollup -c rollup.bin.mjs --silent",
    "inspect": "node ./bin/bin.cjs",
    "qoq:check": "qoq --check --skip-knip",
    "qoq:fix": "qoq --fix --skip-knip",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js": "^1.1.5",
    "eslint-plugin-jest": "28.11.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@saashub/qoq-cli": "^1.1.5",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.16",
    "@vitest/coverage-v8": "3.0.9",
    "rimraf": "6.0.1",
    "rollup": "4.36.0",
    "rollup-plugin-esbuild": "6.2.1",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "peerDependencies": {
    "eslint": "^9.19.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "9d35baaa8cb4bd35d14eddf9862984da831e7079"
}
