{
  "name": "@saashub/qoq-eslint-v9-ts-react",
  "description": "Eslint flat config template for TS + React",
  "license": "MIT",
  "version": "0.11.0",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && rollup -c --silent",
    "test": "vitest run --coverage",
    "qoq:check": "qoq --check --skip-knip",
    "qoq:fix": "qoq --fix --skip-knip",
    "inspect": "npm run build && eslint -c ./lib/eslintConfig.cjs --inspect-config"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-react": "^0.11.0",
    "@saashub/qoq-eslint-v9-ts": "^0.11.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.0",
    "@saashub/qoq-cli": "^0.11.0",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.9",
    "@vitest/coverage-v8": "2.1.1",
    "rimraf": "6.0.1",
    "rollup": "4.22.4",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "keywords": [
    "eslint TypeScript + React template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-react"
  },
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-react",
  "gitHead": "e0e86de03d55c73d4f4bc6a403fd2394c84dc6bf"
}
