{
  "name": "@saashub/qoq-eslint-v9-ts-vitest",
  "version": "1.1.10",
  "description": "Eslint flat config template for TS + Vitest",
  "keywords": [
    "eslint TypeScript + Vitest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-vitest",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-vitest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "bin": {
    "@saashub/qoq-eslint-v9-ts-vitest": "./bin/bin.cjs"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "scripts": {
    "build": "rimraf ./lib && rollup -c rollup.config.mjs --silent && rimraf ./bin && rollup -c rollup.bin.mjs --silent",
    "inspect": "node ./bin/bin.cjs",
    "qoq:check": "qoq --check --skip-knip",
    "qoq:fix": "qoq --fix --skip-knip",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-vitest": "^1.1.10",
    "@saashub/qoq-eslint-v9-ts": "^1.1.10",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.2",
    "@saashub/qoq-cli": "^1.1.10",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.16",
    "@vitest/coverage-v8": "3.1.1",
    "rimraf": "6.0.1",
    "rollup": "4.39.0",
    "typescript": "5.8.3",
    "vitest": "3.1.1"
  },
  "peerDependencies": {
    "eslint": "^9.22.0",
    "typescript": ">=4.8.4 <5.9.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "9d35baaa8cb4bd35d14eddf9862984da831e7079"
}
