{
  "name": "@saashub/qoq-cli",
  "version": "1.0.1",
  "description": "CLI that handles formatting, linting and testing Your JS/TS code",
  "keywords": [
    "cli",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/cli",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "main": "./bin/qoq.cjs",
  "bin": {
    "qoq": "bin/qoq.cjs"
  },
  "scripts": {
    "build": "rimraf ./bin && rollup -c --silent && chmod +x ./bin/qoq.cjs",
    "dev": "npm run build && node ./bin/qoq.cjs",
    "test": "vitest run --coverage",
    "postinstall": "node ./scripts/postinstall.cjs"
  },
  "dependencies": {
    "@antfu/install-pkg": "1.0.0",
    "@eslint/compat": "1.2.6",
    "cac": "6.7.14",
    "local-pkg": "1.0.0",
    "lodash": "4.17.21",
    "micromatch": "4.0.8",
    "picocolors": "1.1.1",
    "prompts": "2.4.2",
    "react-fast-compare": "3.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@saashub/qoq-eslint-v9-js": "^1.0.1",
    "@saashub/qoq-knip": "^1.0.1",
    "@types/lodash": "4.17.15",
    "@types/micromatch": "4.0.9",
    "@types/prompts": "2.4.9",
    "@vitest/coverage-v8": "3.0.5",
    "knip": "5.44.1",
    "rimraf": "6.0.1",
    "rollup": "4.34.6",
    "rollup-plugin-esbuild": "6.2.0",
    "rollup-plugin-typescript-paths": "1.5.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "9d35baaa8cb4bd35d14eddf9862984da831e7079"
}
