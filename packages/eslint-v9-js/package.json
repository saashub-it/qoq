{
  "name": "@saashub/qoq-eslint-v9-js",
  "description": "Eslint flat config template for JS",
  "license": "MIT",
  "version": "0.7.4",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.mjs"
    },
    "./baseConfig": {
      "types": "./lib/baseConfig.d.ts",
      "module": "./lib/baseConfig.mjs",
      "import": "./lib/baseConfig.mjs",
      "require": "./lib/baseConfig.js",
      "default": "./lib/baseConfig.mjs"
    },
    "./eslintConfig": {
      "types": "./lib/eslintConfig.d.ts",
      "module": "./lib/eslintConfig.mjs",
      "import": "./lib/eslintConfig.mjs",
      "require": "./lib/eslintConfig.js",
      "default": "./lib/eslintConfig.mjs"
    },
    "./tools": {
      "types": "./lib/tools.d.ts",
      "module": "./lib/tools.mjs",
      "import": "./lib/tools.mjs",
      "require": "./lib/tools.js",
      "default": "./lib/tools.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && rollup -c --silent",
    "inspect": "npm run build && eslint -c ./lib/eslintConfig.js --inspect-config"
  },
  "dependencies": {
    "@eslint/compat": "1.1.1",
    "@eslint/js": "^9",
    "eslint": "^9",
    "eslint-plugin-compat": "6.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonarjs": "1.0.4",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.7",
    "rimraf": "6.0.1",
    "rollup": "4.21.1",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "overrides": {
    "eslint-plugin-import": {
      "eslint": "$eslint"
    }
  },
  "keywords": [
    "eslint JavaScript template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-js"
  },
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-js"
}
