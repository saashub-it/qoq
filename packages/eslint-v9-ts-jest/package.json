{
  "name": "@saashub/qoq-eslint-v9-ts-jest",
  "description": "Eslint flat config template for TS + Jest",
  "license": "MIT",
  "version": "0.8.0",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    },
    "./baseConfig": {
      "types": "./lib/baseConfig.d.ts",
      "module": "./lib/baseConfig.mjs",
      "import": "./lib/baseConfig.mjs",
      "require": "./lib/baseConfig.cjs",
      "default": "./lib/baseConfig.mjs"
    },
    "./eslintConfig": {
      "types": "./lib/eslintConfig.d.ts",
      "module": "./lib/eslintConfig.mjs",
      "import": "./lib/eslintConfig.mjs",
      "require": "./lib/eslintConfig.cjs",
      "default": "./lib/eslintConfig.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && rollup -c --silent",
    "test": "vitest run --coverage",
    "inspect": "npm run build && eslint -c ./lib/eslintConfig.cjs --inspect-config"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-jest": "^0.8.0",
    "@saashub/qoq-eslint-v9-ts": "^0.8.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.7",
    "rimraf": "6.0.1",
    "rollup": "4.21.2",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "keywords": [
    "eslint TypeScript + Jest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-jest"
  },
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-jest"
}
