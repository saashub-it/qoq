{
  "name": "@saashub/qoq-eslint-v9-ts-jest",
  "version": "1.10.2",
  "description": "Eslint flat config template for TS + Jest",
  "keywords": [
    "eslint TypeScript + Jest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-jest",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-jest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "bin": {
    "@saashub/qoq-eslint-v9-ts-jest": "./bin/bin.cjs"
  },
  "files": [
    "bin",
    "lib",
    "eslint.config.local.js"
  ],
  "scripts": {
    "build": "rimraf ./lib && rollup -c rollup.config.mjs --silent && rimraf ./bin && rollup -c rollup.bin.mjs --silent",
    "inspect": "node ./bin/bin.cjs"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-jest": "^1.10.2",
    "@saashub/qoq-eslint-v9-ts": "^1.10.2",
    "@saashub/qoq-utils": "^1.10.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.4",
    "@types/eslint": "^9",
    "rimraf": "6.0.1",
    "rollup": "4.52.4",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.34.0",
    "typescript": ">=4.8.4 <6.0.0"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "ac2a99d3d6a60c8e5355472d774448b178f35222"
}
