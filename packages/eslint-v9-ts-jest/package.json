{
  "name": "@saashub/qoq-eslint-v9-ts-jest",
  "version": "0.13.2",
  "description": "Eslint flat config template for TS + Jest",
  "keywords": [
    "eslint TypeScript + Jest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-jest",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-jest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "scripts": {
    "build": "rimraf ./lib && rollup -c --silent",
    "inspect": "npm run build && eslint -c ./lib/eslintConfig.cjs --inspect-config",
    "qoq:check": "qoq --check --skip-knip",
    "qoq:fix": "qoq --fix --skip-knip",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-jest": "^0.13.2",
    "@saashub/qoq-eslint-v9-ts": "^0.13.2",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.1",
    "@saashub/qoq-cli": "^0.13.2",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.12",
    "@vitest/coverage-v8": "2.1.3",
    "rimraf": "6.0.1",
    "rollup": "4.24.0",
    "typescript": "5.6.3",
    "vitest": "2.1.3"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e0e86de03d55c73d4f4bc6a403fd2394c84dc6bf"
}
