{
  "name": "@saashub/qoq-eslint-v9-ts-jest",
  "version": "1.7.2",
  "description": "Eslint flat config template for TS + Jest",
  "keywords": [
    "eslint TypeScript + Jest template",
    "flat config",
    "saashub",
    "qoq",
    "QualityOverQuantity"
  ],
  "homepage": "https://github.com/saashub-it/qoq/tree/master/packages/eslint-v9-ts-jest",
  "bugs": {
    "url": "https://github.com/saashub-it/qoq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saashub-it/qoq.git",
    "directory": "packages/eslint-v9-ts-jest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.js",
  "bin": {
    "@saashub/qoq-eslint-v9-ts-jest": "./bin/bin.cjs"
  },
  "scripts": {
    "build": "rimraf ./lib && rollup -c rollup.config.mjs --silent && rimraf ./bin && rollup -c rollup.bin.mjs --silent",
    "inspect": "node ./bin/bin.cjs"
  },
  "dependencies": {
    "@saashub/qoq-eslint-v9-js-jest": "^1.7.2",
    "@saashub/qoq-eslint-v9-ts": "^1.7.2",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.3",
    "@types/eslint": "^9",
    "@types/lodash": "4.17.19",
    "rimraf": "6.0.1",
    "rollup": "4.44.1",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.28.0",
    "typescript": ">=4.8.4 <5.9.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1edc64454abc71aac143d2aa943cbb619245c12b"
}
