    "@types/node": "^20",
    "lerna": "8.1.8"
  },
  "overrides": {
    "eslint-plugin-import": {
      "eslint": "^9"
    },
    "eslint-plugin-react-hooks": {
      "eslint": "^9"
    },
    "eslint-plugin-jsx-a11y": {
      "eslint": "^9"
    }
  },
  "workspaces": [
    "packages/*"
  ]
