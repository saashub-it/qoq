{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "incremental": false,
    "declaration": false,
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
